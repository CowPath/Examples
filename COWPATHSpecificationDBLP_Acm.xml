<scxml initial="COWPATH_state" name="" version="1.0" xmlns="http://www.w3.org/2005/07/scxml" clock="true" notes="true"  previewEnable="false"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<state id="COWPATH_state">
		<initial>
			<transition event="COWPATH:LOAD" cond="http://dblp.uni-trier.de/pers/*" target="http://dblp.uni-trier.de/pers/*"/>
		</initial>
		<state id="http://dblp.uni-trier.de/pers/*" structurePattern="//*[@class='data']">
			<transition event="COWPATH:CLICK" cond="go2acm" target="http://dl.acm.org/results.cfm?*">
				<script>GoTo("http://dl.acm.org/advsearch.cfm?coll=DL&amp;dl=ACM").Script("var title=JSON.stringify(GM_getValue('$title')); GM_setValue('$title',title);").Paste("$title").Into("/html/body/div[1]/table/tbody/tr[2]/td/table/tbody/tr[2]/td[1]/table/tbody/tr[1]/td[2]/input").Click("//input[@name='Go']");</script>
			</transition>
			<onentry>
				<script>Copy("span[last()]").Into("$title");</script>
			</onentry>
		</state>
		<state id="http://dl.acm.org/results.cfm?*"> 
			<transition event="COWPATH:CLICK" cond="acmresult2dblp" target="http://dblp.uni-trier.de/pers/*"/>
			<transition event="COWPATH:LOAD" cond="http://dl.acm.org/citation.cfm?id=*" target="http://dl.acm.org/citation.cfm?id=*"/>
		</state>
		<state id="http://dl.acm.org/citation.cfm?id=*">
			<transition event="COWPATH:CLICK" cond="acm2dblp" target="http://dblp.uni-trier.de/pers/*"/>			
		</state>		
	</state>
</scxml>



