<scxml initial="COWPATH_state" name="" version="1.0" xmlns="http://www.w3.org/2005/07/scxml" clock="true" notes="true" previewEnable="false"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <state id="COWPATH_state">
    <initial>
	  <transition event="COWPATH:LOAD" cond="https://www.easychair.org/conferences/review_all.cgi?*" target="https://www.easychair.org/conferences/review_all.cgi?*"/>
	</initial>
	<state id="https://www.easychair.org/conferences/review_all.cgi?*">			
	  <transition event="COWPATH:LOAD" cond="https://www.easychair.org/conferences/review_add.cgi?*" target="https://www.easychair.org/conferences/review_add.cgi?*"/>
	  <transition event="COWPATH:LOAD" cond="https://www.easychair.org/conferences/submission_info_show.cgi?*" 
						target="https://www.easychair.org/conferences/submission_info_show.cgi?*"/>
	</state>
	<state id="https://www.easychair.org/conferences/review_add.cgi?*" structurePattern="//*[@id='reviewform']//td/b[text()='Authors:']/../../td[2]" contentPattern="and |,">
		<transition event="COWPATH:CLICK" cond="authorPublicationsDBLP" target="http://dblp.uni-trier.de/pers/*">
			<script>GoTo("http://dblp.uni-trier.de/").Paste("$author").Into("//*[@NAME='q']").Click("//*[@VALUE='Submit']");</script>
		</transition>
		<onentry>
			<script>Copy("text()").Into("$author");</script>
		</onentry>
	</state>	
	<state id="http://dblp.uni-trier.de/pers/*">
	  <transition event="COWPATH:CLICK" cond="returnAddReview" target="https://www.easychair.org/conferences/review_add.cgi?*"/>
	</state>
	<state id="https://www.easychair.org/conferences/submission_info_show.cgi?*">
	  <transition event="COWPATH:CLICK" cond="checkAbstractPlagiarismGrammarly" target="https://ed.grammarly.com/register/signup/report_pale/*">
	    <script>GoTo("http://www.grammarly.com/").Paste("$abstract").Into("//*[@ID='maintextarea']").Click("//*[@id='check_btn']");</script>
	  </transition>
	  <onentry>
		<script>Copy("//*[text()='Abstract:']/../td[2]/text()").Into("$abstract");</script>
	  </onentry>
	</state>
	<state id="https://ed.grammarly.com/register/signup/report_pale/*">
	  <transition event="COWPATH:CLICK" cond="returnArticleInformation" target="https://www.easychair.org/conferences/submission_info_show.cgi?*"/>
	</state>		
  </state>
</scxml>





		
	
